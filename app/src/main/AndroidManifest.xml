<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.muhammadasad.fall19">

    <uses-permission android:name="android.permission.INTERNET" />
    <!--  For ForeGround Service -->
    <uses-permission android:name="android.permission.FOREGROUND_SERVICE"/>

    <!-- For job Intent Service-->
    <uses-permission android:name="android.permission.WAKE_LOCK" />

    <application
        android:name=".ApplicationContext"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".services.ServiceActivity"></activity>
        <activity android:name=".Dashboard" />
        <activity android:name=".RegistrationActivity" />
        <activity
            android:name=".SplashActivity"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".recycler.RecyclerViewActivity" />
        <activity android:name=".Main2Activity" />
        <activity android:name=".MainActivity" />

        <service android:name=".services.ForeGroundService"></service>
        <service
            android:name=".services.JobIntentServiceTest"
            android:permission="android.permission.BIND_JOB_SERVICE" />
        <service android:name=".services.IntentServiceTest"></service>
        <service android:name=".services.BoundedService"></service>
    </application>

</manifest>